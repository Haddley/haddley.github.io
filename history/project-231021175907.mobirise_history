 return <h4>Loading...</h4>\n              if (error) console.log({ error })\n              return data.books.map(book =>\n                <>\n                  <h3 key={book.id}>{book.name}</h3>\n                  {book.authors.map(author =>\n                    <h4>{author.name}</h4>)}\n                </>\n              )\n            }\n          }\n        </Query>\n\n      </div>\n\n    </ApolloProvider>\n\n  );\n}\n\nexport default App;\n--></code></pre>\n    \n                </blockquote>\n            </div>\n        </div>\n    </div>\n</section>",
